---
interface Props {
	icon: string;
	url: string
}

const { icon, url } = Astro.props;
---

<div class="col">
  <div class="card">
    <a href={url} target="_blank">
      <div class="card-body text-center">
        <i class={`fa-brands fa-${icon} fa-4x card-icon`}></i>
      </div>
    </a>
  </div>
</div>